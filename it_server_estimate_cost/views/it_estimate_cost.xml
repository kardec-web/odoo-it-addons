<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="it_estimate_cost_form" model="ir.ui.view">
        <field name="name">it.estimate.cost.form</field>
        <field name="model">it.estimate.cost</field>
        <field name="inherit_id" ref="it_base.it_estimate_cost_form" />
        <field name="arch" type="xml">

            <field name="estimate_cost" position="after">
                <field name="server_id"/>
            </field>
        </field>
    </record>

    <record id="it_estimate_cost_search" model="ir.ui.view">
        <field name="name">it.estimate.cost.search</field>
        <field name="model">it.estimate.cost</field>
        <field name="inherit_id" ref="it_base.it_estimate_cost_search" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="server_id"/>
            </field>
        </field>
    </record>
</odoo>
